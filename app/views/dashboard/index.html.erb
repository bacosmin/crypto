<table class="table">
  <thead>
  <tr>
    <th scope="col">Name</th>
    <th scope="col">Cost per transaction</th>
    <th scope="col">Cost per multisig transaction</th>
    <th scope="col">Updated At</th>
  </tr>
  </thead>
  <tbody>
  <% @currencies.each do |c| %>
    <tr>
      <td><%= c.name %></td>
      <td><%= c.transaction_cost || 'N\A' %></td>
      <td><%= c.multisig_transaction_cost || 'N\A'%></td>
      <td><%= c.updated_at%></td>
    </tr>
  <% end %>

  </tbody>
</table>